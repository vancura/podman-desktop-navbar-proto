{
  "name": "podman-desktop-navbar-proto",
  "version": "0.0.1",
  "type": "module",
  "description": "Podman Desktop Navigation Bar prototype using Svelte 5 and Tailwind CSS.",
  "author": "Václav Vančura",
  "license": "ISC",
  "keywords": [
    "podman-desktop",
    "navigation",
    "svelte",
    "tailwind",
    "typescript",
    "prototype"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "biome format --write . && prettier --write \"**/*.{md,mdx,yml,yaml}\"",
    "format:check": "biome check . && prettier --check \"**/*.{md,mdx,yml,yaml}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "husky"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "dependencies": {
    "svelte": "^5.45.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte-check": "^4.3.4",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.7"
  }
}
